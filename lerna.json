{
  "npmClient": "yarn",
  "useWorkspaces": true,
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/!(README).md"
  ],
  "packages": [
    "packages/*"
  ],
  "version": "1.0.0-alpha.7",
  "command": {
    "publish": {
      "allowBranch": "master",
      "message": "chore(release): publish"
    }
  }
}
